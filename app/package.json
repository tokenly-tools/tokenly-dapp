{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:create": "wrangler d1 execute tokenly --local --file=./schema.sql",
    "init:local": "./scripts/init-local-db.sh",
    "db:reset": "wrangler d1 execute tokenly --local --command \"DROP TABLE IF EXISTS _prisma_migrations; DROP TABLE IF EXISTS User;\" && npm run init:local",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@nuxt/eslint": "1.7.0",
    "@prisma/adapter-d1": "^6.11.1",
    "@prisma/client": "6.11.1",
    "@reown/appkit": "^1.7.17",
    "@reown/appkit-adapter-wagmi": "^1.7.17",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/vue-query": "^5.83.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.5.0",
    "@wagmi/vue": "^0.1.25",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.525.0",
    "nuxt": "^3.17.6",
    "reka-ui": "^2.3.2",
    "shadcn-nuxt": "2.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "vee-validate": "^4.15.1",
    "viem": "^2.31.7",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.5.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.31.0",
    "nitro-cloudflare-dev": "^0.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.11.1",
    "sass-embedded": "^1.89.2",
    "vue-eslint-parser": "^10.2.0",
    "wrangler": "^4.24.3"
  }
}
